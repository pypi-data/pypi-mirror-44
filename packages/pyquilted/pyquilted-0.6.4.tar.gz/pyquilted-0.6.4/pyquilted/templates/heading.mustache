{{#heading}}
<div class="section heading">
{{#heading-simple}}
    <div class="line-one">
        <div class="name">{{name}}</div>
        {{#adjacent}}
        <div class="adjacent">{{#title-detail}}{{title}}{{/title-detail}}{{#location-detail}}{{location}}{{#via}}&nbsp;&nbsp;<i class="fa {{.}}"></i>{{/via}}{{/location-detail}}{{#flair-detail}}{{#link}}<a href="{{.}}">{{/link}}<i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}{{#link}}</a>{{/link}}{{/flair-detail}}</div>
        {{/adjacent}}
    </div>
    <div class="line-two">
        {{#primary}}
        <div class="primary">{{#title-detail}}{{title}}{{/title-detail}}{{#location-detail}}{{location}}{{#via}}&nbsp;&nbsp;<i class="fa {{.}}"></i>{{/via}}{{/location-detail}}{{#flair-detail}}{{#link}}<a href="{{.}}">{{/link}}<i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}{{#link}}</a>{{/link}}{{/flair-detail}}</div>
        {{/primary}}
    </div>
{{/heading-simple}}
{{#heading-complex}}
    <div class="line-one">
        <div class="name">{{name}}</div>
        {{#adjacent}}
        <div class="adjacent">{{#title-detail}}{{title}}{{/title-detail}}{{#location-detail}}{{location}}{{#via}}&nbsp;&nbsp;<i class="fa {{.}}"></i>{{/via}}{{/location-detail}}{{#flair-detail}}{{#link}}<a href="{{.}}">{{/link}}<i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}{{#link}}</a>{{/link}}{{/flair-detail}}</div>
        {{/adjacent}}
        {{#top_side}}
        <div class="top-side">{{#title-detail}}{{title}}{{/title-detail}}{{#location-detail}}{{location}}{{#via}}&nbsp;&nbsp;<i class="fa {{.}}"></i>{{/via}}{{/location-detail}}{{#flair-detail}}{{#link}}<a href="{{.}}">{{/link}}{{flair}}&nbsp;&nbsp;<i class="fa {{icon}}"></i>{{#link}}</a>{{/link}}{{/flair-detail}}</div>
        {{/top_side}}
    </div>
    <div class="line-two">
        {{#primary}}
        <div class="primary">{{#objective-detail}}{{objective}}{{/objective-detail}}{{#title-detail}}{{title}}{{/title-detail}}{{#location-detail}}{{location}}{{#via}}&nbsp;&nbsp;<i class="fa {{.}}"></i>{{/via}}{{/location-detail}}{{#flair-detail}}{{#link}}<a href="{{.}}">{{/link}}<i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}{{#link}}</a>{{/link}}{{/flair-detail}}</div>
        {{/primary}}
        {{#bottom_side}}
        <div class="bottom-side">{{#title-detail}}{{title}}{{/title-detail}}{{#location-detail}}{{location}}{{#via}}&nbsp;&nbsp;<i class="fa {{.}}"></i>{{/via}}{{/location-detail}}{{#flair-detail}}{{#link}}<a href="{{.}}">{{/link}}{{flair}}&nbsp;&nbsp;<i class="fa {{icon}}"></i>{{#link}}</a>{{/link}}{{/flair-detail}}</div>
        {{/bottom_side}}
    </div>
{{/heading-complex}}
</div>
{{/heading}}
