<html>
    <head>
        <meta charset="utf-8">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        {{#style}}
        <style>
            body {
                margin: 0;
                padding: 0;
                border-spacing: 0;
                border-collapse: collapse;
            }
            .resume {
                height: {{page_height}};
                width: {{page_width}};
                display: -webkit-box;
                -webkit-box-orient: vertical;
                -webkit-box-pack: justify;

                font-family: {{font_main}};
                font-weight: normal;
                font-size: {{font_size}};
                color: #333333;
                line-height: 200%;
                letter-spacing: normal;
                white-space: nowrap;
            }
            .resume i {
                color: #666666;
            }
            .heading {
                font-family: {{font_other}};
                font-weight: normal;
                font-size: 90%;
                color: #333333;
                line-height: 175%;
                letter-spacing: normal;
                text-align: center;
            }
            .heading > div > div {
                display: inline-block;
            }
            .name {
                color: {{name_color}};
                font-size: 140%;
                font-weight: bold;
            }
            .adjacent {
                margin-left: 12pt;
                color: #666666;
            }
            {{#heading_complex}}
            .heading {
                text-align: left;
            }
            .name {
                float: left;
            }
            .adjacent {
                float: left;
            }
            .top-side {
                float: right;
            }
            .primary {
                clear: both;
                width: 50%;
                float: left;
                white-space: normal;
            }
            .bottom-side {
                float: right;
            }
            {{/heading_complex}}
            .label-icon {
                display:inline-block;
                min-width: 24pt;
            }
            .label {
                font-size: 110%;
                line-height: 100%;
            }
            .block {
                margin-top: 12pt;
                margin-left: 24pt;
            }
            .section.group {
                line-height: 300%;
            }
            .activity-name {
                display: inline-block;
                font-size: 110%;
                float: left;
            }
            .activity-flair{
                display: inline-block;
                float: right;
                color: #666666;
            }
            .activity-description {
                clear: both;
            }
            .activity-slug {
                margin-left: 24pt;
            }
            .job-date {
                display: inline-block;
                float: left;
                color: #666666;
            }
            .job-location {
                display: inline-block;
                float: right;
                color: #666666;
            }
            .job-description {
                clear: both;
            }
            .job-slug{
                margin-left: 24pt;
            }
            .contact {
                font-family: {{font_other}};
                font-weight: normal;
                font-size: 90%;
                color: #666666;
                display: -webkit-box;
                -webkit-box-orient: horizontal;
                -webkit-box-pack: justify;
            }
        </style>
        {{/style}}
    </head>
    <body>
        <div class="resume">

        {{#heading}}
        <div class="section heading">
        {{#heading-simple}}
            <div>
                <div class="name">{{name}}</div>
                {{#adjacent}}
                {{#title-detail}}<div class="adjacent">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="adjacent">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="adjacent"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
                {{/adjacent}}
            </div>
            <div>
                {{#primary}}
                {{#title-detail}}<div class="primary">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="primary">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="primary"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
                {{/primary}}
            </div>
        {{/heading-simple}}
        {{#heading-complex}}
            <div>
                <div class="name">{{name}}</div>
                {{#adjacent}}
                {{#title-detail}}<div class="adjacent">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="adjacent">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="adjacent"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
                {{/adjacent}}
                {{#top_side}}
                {{#title-detail}}<div class="top-side">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="top-side">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="top-side">{{flair}}&nbsp;&nbsp;<i class="fa {{icon}}"></i></div>{{/flair-detail}}
                {{/top_side}}
            </div>
            <div>
                {{#primary}}
                {{#objective-detail}}<div class="primary">{{objective}}</div>{{/objective-detail}}{{#title-detail}}<div class="primary">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="primary">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="primary"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
                {{/primary}}
                {{#bottom_side}}
                {{#title-detail}}<div class="bottom-side">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="bottom-side">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="bottom-side">{{flair}}&nbsp;&nbsp;<i class="fa {{icon}}"></i></div>{{/flair-detail}}
                {{/bottom_side}}
            </div>
        {{/heading-complex}}
        </div>
        {{/heading}}

        {{#sections}}

        {{#skills}}{{>skills}}{{/skills}}
        {{#education}}{{>education}}{{/education}}
        {{#work}}{{>work}}{{/work}}
        {{#projects}}{{>projects}}{{/projects}}

        {{/sections}}

        {{>contacts}}
        </div>
    </body>
</html>
