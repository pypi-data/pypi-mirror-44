<div class="section projects">
    <div class="label">
        <div class="label-icon"><i class="fa {{icon}}"></i></div>{{label}}
    </div>
    {{#blocks}}
    <div class="block projects">
        <div class="activity">
        {{#link}}<a href="{{.}}">{{/link}}
        {{#icon}}<i class="fa {{.}}"></i>&nbsp;&nbsp;{{/icon}}{{name}}&mdash;{{description}}</div>
        {{#link}}</a>{{/link}}
        {{#slugs}}
        <div class="activity-slug">&mdash;{{.}}</div>
        {{/slugs}}
    </div>
    {{/blocks}}
</div>
