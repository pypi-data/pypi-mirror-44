<div class="section work">
    <div class="label">
        <div class="label-icon"><i class="fa {{icon}}"></i></div>{{label}}
    </div>
    {{#blocks}}
    <div class="block work">
        <div class="job-date">{{dates}}</div>
        <div class="job-location">{{location}}</div>
        <div class="job-description">{{title}}, {{company}}</div>
        {{#slugs}}
        <div class="job-slug">&mdash;{{.}}</div>
        {{/slugs}}
    </div>
    {{/blocks}}

    {{#slugs}}
    <div class="block group">
        {{#blocks}}
        <div class="slug">&mdash;{{.}}</div>
        {{/blocks}}
    </div>
    {{/slugs}}
</div>
