{{#heading}}
<div class="section heading">
{{#heading-simple}}
    <div>
        <div class="name">{{name}}</div>
        {{#adjacent}}
        {{#title-detail}}<div class="adjacent">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="adjacent">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="adjacent"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
        {{/adjacent}}
    </div>
    <div>
        {{#primary}}
        {{#title-detail}}<div class="primary">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="primary">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="primary"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
        {{/primary}}
    </div>
{{/heading-simple}}
{{#heading-complex}}
    <div>
        <div class="name">{{name}}</div>
        {{#adjacent}}
        {{#title-detail}}<div class="adjacent">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="adjacent">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="adjacent"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
        {{/adjacent}}
        {{#top_side}}
        {{#title-detail}}<div class="top-side">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="top-side">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="top-side">{{flair}}&nbsp;&nbsp;<i class="fa {{icon}}"></i></div>{{/flair-detail}}
        {{/top_side}}
    </div>
    <div>
        {{#primary}}
        {{#objective-detail}}<div class="primary">{{objective}}</div>{{/objective-detail}}{{#title-detail}}<div class="primary">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="primary">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="primary"><i class="fa {{icon}}"></i>&nbsp;&nbsp;{{flair}}</div>{{/flair-detail}}
        {{/primary}}
        {{#bottom_side}}
        {{#title-detail}}<div class="bottom-side">{{title}}</div>{{/title-detail}}{{#location-detail}}<div class="bottom-side">{{location}}&nbsp;&nbsp;<i class="fa {{via}}"></i></div>{{/location-detail}}{{#flair-detail}}<div class="bottom-side">{{flair}}&nbsp;&nbsp;<i class="fa {{icon}}"></i></div>{{/flair-detail}}
        {{/bottom_side}}
    </div>
{{/heading-complex}}
</div>
{{/heading}}
