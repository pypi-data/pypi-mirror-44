#!/usr/local/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'cliifs==0.5','console_scripts','cliifs'
__requires__ = 'cliifs==0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cliifs==0.5', 'console_scripts', 'cliifs')()
    )
