            datetime.date(2016, 8, 11), datetime.date(2016, 11, 3), datetime.date(2016, 9, 22), 
            datetime.date(2016, 10, 10), datetime.date(2016, 12, 23), datetime.date(2016, 5, 5), 
            datetime.date(2016, 9, 19)}
        
