#, python-format
msgid "Illegal url for %%ksappend: %s"
msgstr "Niedozwolony adres URL dla %%ksappend: %s"

#: ../pykickstart/parser.py:95
#, python-format
msgid "Unable to open %%ksappend file: %s"
msgstr "Nie można otworzyć pliku %%ksappend: %s"

